{
    "jsc": {
        "target": "es5",
        "keepClassNames": true,
        "externalHelpers": true,
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true,
            "classPrivateProperty": true
        }
    },
    "module": {
        "type": "commonjs"
    },
    "sourceMaps": true,
    "exclude": [".*\\.spec|mock\\.tsx?$"]
}
