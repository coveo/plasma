{
    "name": "@coveord/plasma-react-legacy-charts",
    "version": "48.23.2",
    "description": "Plasma charts implemented with React!",
    "keywords": [
        "coveo",
        "plasma",
        "css",
        "react",
        "charts",
        "front-end",
        "web"
    ],
    "homepage": "https://github.com/coveo/plasma#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/plasma.git"
    },
    "license": "Apache-2.0",
    "author": "Coveo",
    "exports": {
        ".": {
            "types": "./dist/definitions/Entry.d.ts",
            "import": "./dist/esm/Entry.js",
            "require": "./dist/cjs/Entry.js"
        }
    },
    "main": "./dist/cjs/Entry.js",
    "module": "./dist/esm/Entry.js",
    "types": "dist/definitions/Entry.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "node ../../scripts/build",
        "clean": "rimraf dist coverage",
        "lintfix": "../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint -c ./.eslintrc.js \"src/**/*.{ts,tsx}\" --fix",
        "start": "node ../../scripts/start",
        "test": "jest --maxWorkers=65%",
        "test:debug": "node --inspect-brk node_modules/jest/bin/jest --runInBand --watchAll --detectOpenHandles --forceExit",
        "test:watch": "jest --watchAll"
    },
    "dependencies": {
        "@coveord/plasma-react": "workspace:*",
        "@swc/helpers": "0.4.12",
        "classnames": "2.3.1",
        "d3": "7.6.1",
        "d3-array": "3.2.0",
        "d3-scale": "4.0.2",
        "d3-shape": "3.1.0",
        "underscore": "1.13.1"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "17.0.2",
        "@swc/cli": "0.1.57",
        "@swc/core": "1.3.22",
        "@swc/jest": "0.2.23",
        "@testing-library/dom": "8.18.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/classnames": "2.3.0",
        "@types/d3": "7.4.0",
        "@types/d3-array": "3.0.3",
        "@types/d3-scale": "4.0.2",
        "@types/d3-shape": "3.1.0",
        "@types/enzyme": "3.10.12",
        "@types/jest": "29.0.3",
        "@types/react": "18.0.21",
        "@types/testing-library__jest-dom": "5.14.5",
        "@types/underscore": "1.8.10",
        "@typescript-eslint/eslint-plugin": "5.21.0",
        "@typescript-eslint/parser": "5.21.0",
        "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
        "codecov": "3.8.2",
        "enzyme": "3.11.0",
        "eslint-plugin-jest": "27.0.4",
        "eslint-plugin-jest-dom": "4.0.2",
        "eslint-plugin-testing-library": "5.3.1",
        "fancy-log": "2.0.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.0.3",
        "jest-environment-jsdom": "29.0.3",
        "lint-staged": "9.5.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "regenerator-runtime": "0.13.9",
        "rimraf": "3.0.2",
        "sass": "1.51.0",
        "ts-jest": "29.0.1",
        "tslib": "2.3.1",
        "typescript": "4.9.3"
    },
    "peerDependencies": {
        "@types/react": "^18.0",
        "@types/react-dom": "^18.0",
        "react": "^18.0",
        "react-dom": "^18.0"
    }
}
