{
    "name": "react-vapor",
    "version": "30.7.0",
    "description": "Vapor CSS components implemented with React!",
    "keywords": [
        "coveo",
        "vapor",
        "css",
        "react",
        "front-end",
        "web"
    ],
    "homepage": "https://github.com/coveo/react-vapor#readme",
    "author": "Coveo",
    "license": "Apache-2.0",
    "main": "dist/cjs/Entry.js",
    "module": "dist/esm/Entry.js",
    "types": "dist/definitions/Entry.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/react-vapor.git"
    },
    "scripts": {
        "build": "tsc --build tsconfig.build.json && tsc --build tsconfig.esm.json",
        "test": "jest --maxWorkers=65%",
        "test:ci": "jest --maxWorkers=2",
        "test:watch": "jest --watchAll",
        "test:debug": "jest --clearCache && node --inspect node_modules/jest/bin/jest --runInBand --watchAll",
        "report-coverage": "codecov -f coverage/lcov.info",
        "precommit": "lint-staged",
        "snyk-protect": "npx snyk protect",
        "prepublish": "npm run snyk-protect",
        "clean": "rimraf dist coverage",
        "lintfix": "../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint -c ./.eslintrc.js \"src/**/*.{ts,tsx}\" --fix",
        "postversion": "find ./dist -type f -exec sed -i '' -e 's,||||VERSION||||,'\"$npm_package_version\"',g' {} \\;"
    },
    "dependencies": {
        "@types/codemirror": "0.0.109",
        "chosen-js": "1.8.7",
        "classnames": "2.3.1",
        "coveo-styleguide": "^30.7.0",
        "dnd-core": "2.6.0",
        "moment": "2.29.1",
        "query-string": "6.14.1",
        "rc-slider": "9.7.4",
        "react-bootstrap": "2.0.3",
        "react-codemirror2": "7.2.1",
        "react-color": "2.19.3",
        "react-diff-viewer": "^3.1.1",
        "react-dnd": "2.6.0",
        "react-dnd-html5-backend": "2.6.0",
        "react-infinite-scroll-component": "4.5.3",
        "react-modal": "3.14.4",
        "react-tether": "1.0.5",
        "react-textarea-autosize": "8.3.3",
        "react-transition-group": "2.9.0",
        "reselect": "4.1.5",
        "split-on-first": "1.x.x",
        "strict-uri-encode": "2.x.x",
        "unidiff": "0.0.4"
    },
    "peerDependencies": {
        "codemirror": "^5.39.2",
        "d3": "^3.5.17",
        "jquery": ">= 2.0.0 =< 3",
        "react": "^16.14.21",
        "react-dom": "^16.14.0",
        "react-redux": "^7.2.4",
        "redux": "^4.0.1",
        "underscore": "^1.8.3",
        "underscore.string": "^3.3.5"
    },
    "devDependencies": {
        "@helpers/enzyme-redux": "^1.0.0",
        "@testing-library/dom": "8.11.1",
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "12.8.3",
        "@types/chosen-js": "1.8.3",
        "@types/classnames": "2.3.0",
        "@types/d3": "3.5.46",
        "@types/enzyme": "3.10.10",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/escape-string-regexp": "1.0.0",
        "@types/faker": "4.1.12",
        "@types/jest": "27.0.3",
        "@types/jquery": "2.0.57",
        "@types/loadable__component": "5.13.4",
        "@types/lorem-ipsum": "2.0.0",
        "@types/react": "16.14.21",
        "@types/react-bootstrap": "0.32.28",
        "@types/react-codemirror": "1.0.6",
        "@types/react-color": "3.0.6",
        "@types/react-dom": "16.9.14",
        "@types/react-infinite-scroll-component": "4.2.5",
        "@types/react-modal": "3.13.1",
        "@types/react-tether": "0.5.5",
        "@types/react-textarea-autosize": "4.3.6",
        "@types/react-transition-group": "2.9.2",
        "@types/redux-logger": "3.0.9",
        "@types/redux-mock-store": "1.0.3",
        "@types/redux-promise-middleware": "6.0.0",
        "@types/underscore": "1.8.10",
        "@types/underscore.string": "0.0.38",
        "ansi-colors": "4.1.1",
        "autoprefixer": "9.8.8",
        "codecov": "3.8.2",
        "codemirror": "5.63.3",
        "d3": "3.5.17",
        "del": "5.1.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "escape-string-regexp": "1.0.5",
        "eslint-plugin-jest": "24.7.0",
        "eslint-plugin-jest-dom": "3.9.2",
        "eslint-plugin-testing-library": "3.10.2",
        "faker": "4.1.0",
        "fancy-log": "1.3.3",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.3",
        "jquery": "3.6.0",
        "lint-staged": "9.5.0",
        "lorem-ipsum": "2.0.4",
        "minimist": "1.2.5",
        "node-sass": "5.0.0",
        "postcss": "8.4.4",
        "react": "16.14.0",
        "react-dnd-test-backend": "2.6.0",
        "react-dom": "16.14.0",
        "react-redux": "7.2.6",
        "redux": "4.1.2",
        "redux-logger": "3.0.6",
        "redux-mock-store": "1.5.4",
        "redux-promise-middleware": "6.1.2",
        "redux-thunk": "2.4.1",
        "rimraf": "3.0.2",
        "snyk": "1.787.0",
        "ts-jest": "27.1.1",
        "tslib": "1.14.1",
        "typescript": "3.8.3",
        "underscore": "1.13.1",
        "underscore.string": "3.3.5"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "**/*.scss": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ],
        "**/*.{ts,tsx}": [
            "prettier --write",
            "eslint -c ./.eslintrc.js --fix",
            "git add"
        ],
        "**/*.{js,jsx,json,md,yml,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": "tsjs/prettier-config",
    "snyk": true
}
