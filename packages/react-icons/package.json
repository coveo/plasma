{
    "name": "@coveord/plasma-react-icons",
    "version": "56.2.2",
    "description": "Plasma iconography exposed as react components",
    "keywords": [
        "plasma",
        "icons",
        "react"
    ],
    "homepage": "https://github.com/coveo/plasma/packages/react-icons#readme",
    "bugs": {
        "url": "https://github.com/coveo/plasma/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/plasma.git",
        "directory": "packages/react-icons"
    },
    "license": "ISC",
    "author": "",
    "sideEffects": false,
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        },
        "./mock": {
            "require": "./dist/cjs/mock/index.js"
        },
        "./testing-library": {
            "require": "./dist/cjs/testing-library/index.js",
            "import": "./dist/esm/testing-library/index.js"
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "files": [
        "dist/*",
        "mock/index.js"
    ],
    "scripts": {
        "build": "pnpm generate && node ../../scripts/build",
        "clean": "rimraf dist",
        "generate": "node ./bin/index.js",
        "lintfix": "../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint \"**/*.{ts,tsx}\" --fix",
        "prepublishOnly": "publint",
        "test": "pnpm generate && vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@swc/helpers": "0.5.17",
        "@tabler/icons-react": "3.35.0"
    },
    "devDependencies": {
        "@babel/types": "7.28.4",
        "@coveord/plasma-tokens": "workspace:*",
        "@svgr/core": "8.1.0",
        "@svgr/plugin-jsx": "8.1.0",
        "@swc/cli": "0.7.8",
        "@swc/core": "1.13.5",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.8.0",
        "@testing-library/react": "16.3.0",
        "@types/react": "18.3.26",
        "@types/react-dom": "18.3.7",
        "fs-extra": "11.3.2",
        "glob": "11.0.3",
        "jsdom": "25.0.1",
        "lodash.groupby": "4.6.0",
        "lodash.upperfirst": "4.3.1",
        "publint": "0.3.14",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "rimraf": "6.0.1",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "react": ">= 18.0",
        "react-dom": ">= 18.0"
    }
}
