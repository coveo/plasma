{
    "name": "@coveord/plasma-react-icons",
    "version": "47.1.0",
    "description": "Plasma iconography exposed as react components",
    "keywords": [
        "plasma",
        "icons",
        "react"
    ],
    "homepage": "https://github.com/coveo/plasma/packages/react-icons#readme",
    "bugs": {
        "url": "https://github.com/coveo/plasma/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/plasma.git",
        "directory": "packages/react-icons"
    },
    "license": "ISC",
    "author": "",
    "sideEffects": false,
    "main": "./dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "./dist/definitions/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm generate && pnpm compile",
        "generate": "node ./bin/index.js",
        "compile": "run-p compile:*",
        "compile:cjs": "swc ./src --config module.type=commonjs --config jsc.target=es5 -d dist/cjs",
        "compile:esm": "swc ./src --config module.type=es6 --config jsc.target=es2020 -d dist/esm",
        "compile:dts": "tsc -p tsconfig.json --emitDeclarationOnly"
    },
    "dependencies": {
        "@loadable/component": "5.15.2",
        "@swc/helpers": "0.4.11"
    },
    "devDependencies": {
        "@babel/types": "7.17.10",
        "@coveord/plasma-tokens": "workspace:*",
        "@svgr/core": "6.2.1",
        "@swc/cli": "0.1.57",
        "@swc/core": "1.3.3",
        "@types/loadable__component": "5.13.4",
        "@types/react": "17.0.44",
        "@types/react-dom": "17.0.15",
        "fs-extra": "10.1.0",
        "glob": "7.2.0",
        "lodash.groupby": "4.6.0",
        "lodash.upperfirst": "4.3.1",
        "npm-run-all": "4.1.5",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "tslib": "2.3.1",
        "typescript": "4.6.4"
    },
    "peerDependencies": {
        "react": ">= 17.0.2",
        "react-dom": ">= 17.0.2"
    }
}
