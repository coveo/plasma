{
    "name": "@coveord/plasma-react-icons",
    "version": "56.20.0",
    "description": "Plasma iconography exposed as react components",
    "keywords": [
        "plasma",
        "icons",
        "react"
    ],
    "homepage": "https://github.com/coveo/plasma/packages/react-icons#readme",
    "bugs": {
        "url": "https://github.com/coveo/plasma/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/plasma.git",
        "directory": "packages/react-icons"
    },
    "license": "ISC",
    "author": "",
    "sideEffects": false,
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        },
        "./mock": {
            "require": "./dist/cjs/mock/index.js",
            "import": "./dist/esm/mock/index.js"
        },
        "./testing-library": {
            "require": "./dist/cjs/testing-library/index.js",
            "import": "./dist/esm/testing-library/index.js"
        },
        "./vite-plugin": {
            "import": "./dist/esm/vite-plugin/index.js"
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "files": [
        "dist/*",
        "mock/index.js"
    ],
    "scripts": {
        "build": "pnpm generate && node ../../scripts/build",
        "clean": "rimraf dist",
        "generate": "node ./bin/index.js",
        "lintfix": "../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint \"**/*.{ts,tsx}\" --fix",
        "prepublishOnly": "publint",
        "test": "pnpm generate && vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@swc/helpers": "0.5.18",
        "@tabler/icons-react": "3.36.1"
    },
    "devDependencies": {
        "@babel/types": "7.29.0",
        "@coveord/plasma-tokens": "workspace:*",
        "@svgr/core": "8.1.0",
        "@svgr/plugin-jsx": "8.1.0",
        "@swc/cli": "0.7.10",
        "@swc/core": "1.15.11",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/react": "16.3.2",
        "@types/node": "24.10.9",
        "@types/react": "19.2.10",
        "@types/react-dom": "19.2.3",
        "fs-extra": "11.3.3",
        "glob": "13.0.0",
        "jsdom": "27.4.0",
        "lodash.groupby": "4.6.0",
        "lodash.upperfirst": "4.3.1",
        "publint": "0.3.17",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "rimraf": "6.1.2",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "vite": "7.3.1",
        "vitest": "4.0.18"
    },
    "peerDependencies": {
        "react": ">= 18.0.0 < 20.0.0",
        "react-dom": ">= 18.0.0 < 20.0.0"
    }
}
