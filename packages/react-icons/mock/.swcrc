{
    "$schema": "https://json.schemastore.org/swcrc",
    "module": {
        "type": "commonjs"
    },
    "jsc": {
        "target": "es2020",
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false,
            "dynamicImport": false
        },
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        }
    },
    "sourceMaps": "inline"
}
