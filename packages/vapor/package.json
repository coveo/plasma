{
  "name": "coveo-styleguide",
  "version": "3.10.0",
  "description": "Yet another CSS framework - but it's awesome & built by Coveo.",
  "keywords": [
    "coveo",
    "styleguide",
    "css",
    "front-end",
    "web"
  ],
  "homepage": "https://coveo.github.io/styleguide",
  "author": "Coveo",
  "license": "MIT",
  "main": "dist/js/VaporSVG",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp docs",
    "buildpages": "bundle exec jekyll build",
    "start": "concurrently \"npm run build && bundle exec jekyll server --host=0.0.0.0\" \"gulp watch\"",
    "lint": "csscomb scss/**/*.scss",
    "version": "gulp clean && npm run build",
    "reconstruct": "gulp clean --all && npm install && gulp",
    "lintfix": "node node_modules/tsjs --all --exclude tmp/**/*.js docs/**/*.js lib/**/*.js resources/**/*.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Coveo/styleguide.git"
  },
  "dependencies": {
    "chosen-npm": "1.4.2",
    "coveo-slider": "1.0.1",
    "materialize-css": "0.98.2"
  },
  "devDependencies": {
    "ansi-colors": "3.2.4",
    "autosize": "3.0.21",
    "concurrently": "4.0.1",
    "coveo-png-sprite": "1.0.1",
    "csscomb": "4.2.0",
    "del": "3.0.0",
    "fancy-log": "1.3.3",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "5.0.0",
    "gulp-cheerio": "0.6.3",
    "gulp-clean-css": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-file-insert": "2.0.0",
    "gulp-files-to-json": "0.2.1",
    "gulp-gzip": "1.4.1",
    "gulp-if": "2.0.2",
    "gulp-json-sort": "1.0.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgmin": "1.2.4",
    "gulp-uglify": "3.0.0",
    "merge-stream": "1.0.1",
    "minimist": "1.2.0",
    "plugin-error": "1.0.1",
    "pre-commit": "1.2.2",
    "tsjs": "0.7.x",
    "underscore": "1.9.1",
    "underscore.string": "3.3.5"
  },
  "files": [
    "dist",
    "gulpTasks",
    "scss/**/*.scss",
    "lib",
    "resources",
    "gulpfile.js",
    "LICENSE"
  ]
}
