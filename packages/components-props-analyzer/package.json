{
    "name": "@coveord/plasma-components-props-analyzer",
    "version": "52.14.1",
    "private": true,
    "description": "",
    "keywords": [],
    "license": "Apache-2.0",
    "author": "Coveo",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "scripts": {
        "build": "pnpm gen:props && pnpm prettify && pnpm compile",
        "clean": "rimraf dist",
        "compile": "node ../../scripts/build",
        "gen:props": "ts-node --project ./bin/tsconfig.json ./bin/index.ts",
        "lintfix": "../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint -c ./.eslintrc.js \"src/**/*.{ts,tsx}\" --fix",
        "prettify": "prettier --write 'src/components/*.ts'",
        "start": "nodemon"
    },
    "dependencies": {
        "@coveord/plasma-mantine": "workspace:*",
        "@coveord/plasma-react": "workspace:*"
    },
    "devDependencies": {
        "@swc/cli": "0.1.62",
        "@swc/core": "1.3.78",
        "@types/fs-extra": "11.0.1",
        "@types/lz-string": "1.3.34",
        "@types/node": "18.17.5",
        "@types/react": "18.2.20",
        "@types/react-dom": "18.2.7",
        "@types/rimraf": "3.0.2",
        "@typescript/twoslash": "3.2.4",
        "@typescript/vfs": "1.5.0",
        "cross-fetch": "4.0.0",
        "fs-extra": "11.1.1",
        "nodemon": "3.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "5.0.1",
        "ts-node": "10.9.1",
        "tslib": "2.6.1",
        "typescript": "5.1.6"
    }
}
