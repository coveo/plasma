{
    "name": "react-vapor-demo",
    "version": "30.4.0",
    "private": true,
    "description": "Vapor Design System",
    "main": "src/Index.ts",
    "scripts": {
        "start": "tsc --build && concurrently \"tsc --build --watch\" \"webpack-dev-server\"",
        "build": "tsc --build && webpack",
        "precommit": "lint-staged",
        "snyk-protect": "npx snyk protect",
        "prepublish": "npm run snyk-protect",
        "clean": "rimraf dist built",
        "lintfix": "../../node_modules/.bin/stylelint --fix \"**/*.scss\" && ../../node_modules/.bin/prettier --write \"**/*.{scss,ts,tsx,js,jsx,json,md,yml,html}\" && ../../node_modules/.bin/eslint -c ./.eslintrc.js \"src/**/*.{ts,tsx}\" --fix"
    },
    "author": "Coveo",
    "license": "Apache-2.0",
    "homepage": "https://vapor.coveo.com/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coveo/react-vapor.git"
    },
    "dependencies": {
        "classnames": "2.3.1",
        "codemirror": "5.63.3",
        "coveo-styleguide": "^30.4.0",
        "d3": "3.5.17",
        "faker": "4.1.0",
        "jquery": "3.6.0",
        "lorem-ipsum": "2.0.4",
        "moment": "2.29.1",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-markdown": "5.0.3",
        "react-redux": "7.2.6",
        "react-router": "6.0.2",
        "react-router-dom": "6.0.2",
        "react-syntax-highlighter": "15.4.5",
        "react-vapor": "^30.4.0",
        "redux": "4.1.2",
        "redux-devtools-extension": "2.13.9",
        "redux-promise-middleware": "6.1.2",
        "redux-thunk": "2.4.1",
        "reselect": "4.1.5",
        "underscore": "1.13.1",
        "underscore.string": "3.3.5"
    },
    "devDependencies": {
        "@loadable/component": "5.15.0",
        "@types/classnames": "2.3.0",
        "@types/faker": "4.1.12",
        "@types/loadable__component": "5.13.4",
        "@types/lorem-ipsum": "2.0.0",
        "@types/prettier": "1.19.1",
        "@types/react": "16.14.21",
        "@types/react-dom": "16.9.14",
        "@types/react-router": "5.1.17",
        "@types/react-router-dom": "5.3.2",
        "@types/react-syntax-highlighter": "13.5.2",
        "@types/redux-promise-middleware": "6.0.0",
        "@types/underscore": "1.8.10",
        "@types/webpack-env": "1.16.3",
        "concurrently": "6.3.0",
        "copy-webpack-plugin": "6.4.1",
        "css-loader": "3.6.0",
        "file-loader": "5.1.0",
        "html-webpack-plugin": "4.5.2",
        "html-webpack-tags-plugin": "2.0.17",
        "lint-staged": "9.5.0",
        "node-sass": "5.0.0",
        "raw-loader": "4.0.2",
        "rimraf": "3.0.2",
        "sass": "1.44.0",
        "sass-loader": "10.2.0",
        "snyk": "1.786.0",
        "source-map-loader": "1.1.3",
        "style-loader": "1.3.0",
        "tslib": "1.14.1",
        "typescript": "3.8.3",
        "webpack": "4.46.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.3"
    },
    "lint-staged": {
        "**/*.scss": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ],
        "**/*.{ts,tsx}": [
            "prettier --write",
            "eslint -c ./.eslintrc.js --fix",
            "git add"
        ],
        "**/*.{js,jsx,json,md,yml,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "snyk": true
}
