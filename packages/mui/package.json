        "@types/faker": "4.1.12",
        "@types/jest": "27.4.1",
        "@types/jquery": "2.0.60",
        "@types/loadable__component": "5.13.4",
        "@types/lorem-ipsum": "2.0.0",
        "@types/react": "17.0.44",
        "@types/react-bootstrap": "0.32.29",
        "@types/react-codemirror": "1.0.7",
        "@types/react-color": "3.0.6",
