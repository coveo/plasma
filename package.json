{
  "name": "react-vapor",
  "version": "0.7.13",
  "description": "Vapor CSS components implemented with React!",
  "keywords": [
    "coveo",
    "vapor",
    "css",
    "react",
    "front-end",
    "web"
  ],
  "homepage": "https://github.com/coveo/react-vapor#readme",
  "author": "Coveo",
  "license": "MIT",
  "main": "dist/react-vapor.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/react-vapor.git"
  },
  "scripts": {
    "start": "gulp docs && webpack-dev-server --devtool eval --inline --hot --progress --colors",
    "lint": "gulp prettify",
    "build": "gulp",
    "pretest": "gulp clean:tests",
    "test": "node --max-old-space-size=2048 node_modules/karma/bin/karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,karma.entry.ts",
    "coverage:report": "istanbul report",
    "prepublish": "npm run build && npm test",
    "report-coverage": "codecov -f coverage/coverage.json"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "react-tether": "0.5.5"
  },
  "peerDependencies": {
    "jquery": ">= 2.0.0 =< 3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
