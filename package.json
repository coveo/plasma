  "author": "Coveo",
  "license": "MIT",
  "main": "dist/react-vapor.js",
  "types": "dist/react-vapor.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/react-vapor.git"
  },
  "scripts": {
    "start": "npm run docs && webpack-dev-server --mode development --config webpack.config.js --host 0.0.0.0 --devtool eval --inline --hot --progress --colors",
    "docs": "webpack --config webpack.config.js",
    "lintfix": "node node_modules/tsjs -a -n -e coverage/**/*.js docs/**/*.js **/*css.d.ts",
    "compileTs": "webpack --config webpack.config.prod.js",
    "minify": "webpack --config webpack.config.prod.minify.js",
    "build": "gulp clean && npm run compileTs && npm run minify && gulp ts:definitions",
    "pretest": "gulp clean:tests",
    "test": "karma start",
    "test:compile": "webpack --display-error-details --config webpack.config.test.js",
    "test:watch": "node --max-old-space-size=2048 node_modules/karma/bin/karma start --no-single-run",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,karma.entry.ts",
    "coverage:report": "istanbul report",
    "version": "gulp clean --all && npm install && npm run build && npm test",
    "report-coverage": "codecov -f coverage/coverage.json",
    "reconstruct": "gulp clean --all && npm install && npm run docs"
  },
  "pre-commit": [
    "lintfix"
  ],
