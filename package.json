{
  "name": "react-vapor",
  "version": "0.1.3",
  "description": "Vapor CSS components implemented with React!",
  "keywords": [
    "coveo",
    "vapor",
    "css",
    "react",
    "front-end",
    "web"
  ],
  "homepage": "https://github.com/coveo/react-vapor#readme",
  "author": "Coveo",
  "license": "MIT",
  "main": "dist/react-vapor.js",
  "typings": "dist/react-vapor.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/react-vapor.git"
  },
  "scripts": {
    "start": "gulp docs && webpack-dev-server --devtool eval --inline --hot --progress --colors",
    "lint": "gulp prettify",
    "build": "gulp",
    "pretest": "gulp clean:tests",
    "test": "node --max-old-space-size=2048 node_modules/karma/bin/karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report && rm -rf coverage/lcov.info",
    "coverage:remap": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-final.json -t json -e node_modules,tests,karma.entry.ts",
    "coverage:report": "istanbul report",
    "prepublish": "npm run build && npm test",
    "report-coverage": "codecov"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "react-tether": "0.5.2"
  },
  "peerDependencies": {
    "tether": "^1.3.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "@types/backbone": "1.3.33",
    "@types/enzyme": "2.4.34",
    "@types/jasmine": "2.2.34",
    "@types/jquery": "2.0.32",
    "@types/react": "0.14.36",
    "@types/react-dom": "0.14.17",
    "@types/react-redux": "4.4.32",
    "@types/redux": "3.5.30",
    "@types/tether": "1.1.27",
    "@types/underscore": "1.7.33",
    "@types/webpack": "1.12.34",
    "autoprefixer": "6.4.1",
    "backbone": "1.3.3",
    "codecov": "^1.0.1",
    "coveo-styleguide": "1.5.1",
    "css-loader": "0.25.0",
    "del": "2.2.2",
    "dts-generator": "1.7.0",
    "enzyme": "2.4.1",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "gulp-insert": "0.5.0",
    "gulp-replace": "0.5.4",
    "gulp-sequence": "0.4.6",
    "gulp-util": "3.0.7",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jquery": "3.1.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "merge-stream": "1.0.0",
    "node-sass": "3.10.0",
    "postcss-loader": "0.13.0",
    "pre-commit": "1.1.3",
    "pretty-typescript": "1.13.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "remap-istanbul": "0.6.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "underscore": "1.8.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "gulpfile.js",
    "webpack.prod.config.js",
    "LICENSE"
  ]
}
