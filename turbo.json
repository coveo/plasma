{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**", "out/**"],
            "outputMode": "new-only"
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": [],
            "inputs": ["src/**/*.spec.*"],
            "outputMode": "new-only"
        }
    }
}
