{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"],
            "outputLogs": "new-only"
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": [],
            "inputs": ["src/**/*.spec.*"],
            "outputLogs": "new-only"
        }
    }
}
